<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/black"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mainactivity_board"
        android:layout_width="354dp"
        android:layout_height="445dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:background="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/mainactivity_player_view_turn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/mainactivity_q1"
            android:layout_width="112dp"
            android:layout_height="136dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q4"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q2"
            android:layout_width="108dp"
            android:layout_height="139dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q6"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q3"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q3"
            android:layout_width="98dp"
            android:layout_height="133dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q2"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q4"
            android:layout_width="120dp"
            android:layout_height="139dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q7"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q5"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q1"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q5"
            android:layout_width="110dp"
            android:layout_height="129dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q8"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q6"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q4"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q2"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q6"
            android:layout_width="111dp"
            android:layout_height="126dp"
            app:layout_constraintBottom_toTopOf="@+id/mainactivity_q9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q5"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q3"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q7"
            android:layout_width="105dp"
            android:layout_height="132dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q8"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q4"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q8"
            android:layout_width="116dp"
            android:layout_height="126dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/mainactivity_q9"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q7"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q5"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_q9"
            android:layout_width="111dp"
            android:layout_height="145dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mainactivity_q8"
            app:layout_constraintTop_toBottomOf="@+id/mainactivity_q6"
            app:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/mainactivity_winner_line"
            android:layout_width="394dp"
            android:layout_height="465dp"
            android:scaleType="fitXY"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/mainactivity_q8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/empty" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/mainactivity_player_view_turn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainactivity_board"
        app:srcCompat="@drawable/oplay" />
</androidx.constraintlayout.widget.ConstraintLayout>